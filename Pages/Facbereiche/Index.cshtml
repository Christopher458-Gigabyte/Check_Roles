@page
@model Check_Roles.Pages.Facbereiche.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fachbereich[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fachbereich[0].AdminId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fachbereich[0].Studiengänge)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Fachbereich) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AdminId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Studiengänge)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.FachbereichId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.FachbereichId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.FachbereichId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
